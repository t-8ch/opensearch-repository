<?xml version="1.0"?>
<!-- https://en.wikipedia.org/w/opensearch_desc.php -->
<OpenSearchDescription xmlns="http://a9.com/-/spec/opensearch/1.1/"
                       xmlns:moz="http://www.mozilla.org/2006/browser/search/">
	<ShortName>Wikipedia (en)</ShortName>
	<Description>Wikipedia (en)</Description>
	<Url type="text/html" method="GET" template="https://en.wikipedia.org/w/index.php">
		<Param name="search" value="{searchTerms}" />
		<Param name="title" value="Special:Search" />
	</Url>
	<Url type="application/x-suggestions+json" method="GET" template="https://en.wikipedia.org/w/api.php" >
		<Param name="search" value="{searchTerms}" />
		<Param name="action" value="opensearch" />
		<Param name="namespace" value="0" />
	</Url>
	<Url type="application/x-suggestions+xml" method="GET" template="https://en.wikipedia.org/w/api.php">
		<Param name="search" value="{searchTerms}" />
		<Param name="format" values="xml" />
		<Param name="action" value="opensearch" />
		<Param name="namespace" value="0" />
	</Url>
	<moz:SearchForm>https://en.wikipedia.org/wiki/Special:Search</moz:SearchForm>
	<Image height="16" width="16">data:image/x-icon;base64,</Image>
</OpenSearchDescription>
